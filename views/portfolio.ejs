<section class="portfolio-section spad pb-0">
  <div class="container-fluid">
    <div class="row">
      <div class="col-xl-4 col-md-8 offset-xl-2">
        <div class="section-title">
          <h2>Portfolio</h2>
        </div>
      </div>
      <div class="col-md-4 text-md-right">
        <a href="#" class="site-btn mb-5">See All Portfolio</a>
      </div>
    </div>
    <div class="portfolio-warp">
      <div class="row">
        <% portfoliolist.forEach(function(portfolio){ %>
        <div class="col-xl-3 col-lg-6 col-md-6">
          <div class="portfolio-item">
            <a
              href="img/portfolio/<%- portfolio.image %>"
              class="set-bg port-pic"
              data-setbg="img/portfolio/<%- portfolio.image %>"
            ></a>
            <h2><%- portfolio.mission %></h2>
            <p><%= portfolio.descr %></p>
          </div>
        </div>
        <% }); %>
      </div>
    </div>
  </div>
</section>
