<section class="resume-section with-bg spad">
  <div class="container-fluid">
    <div class="row">
      <div class="col-xl-7 offset-xl-2">
        <div class="section-title">
          <h2>Education</h2>
        </div>
        <ul class="resume-list">
          <% educationlist.forEach(function(education) { %>
          <li>
            <h2><%= education.year %></h2>
            <h3><%= education.education %></h3>
            <h4><%= education.college %></h4>
            <p><%= education.descr %></p>
          </li>
          <% }); %>
        </ul>
      </div>
    </div>
  </div>
</section>
